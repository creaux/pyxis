    "build": "tsc && cp ./src/parts/ts/tsconfig.json ./build/parts/ts/ && cp ./src/parts/ts/babelrc/babelrc.json ./build/parts/ts/babelrc/",
    "build:watch": "tsc --watch",
    "test": "nyc ava --verbose",
    "test:watch": "ava --watch",
    "precommit": "lint-staged",
    "report": "nyc report",
    "lint": "tslint -c tslint.json ./src/**/*.ts",
    "postinstall": "yarn build",
    "loader:higher": "nodemon --watch ./src/**/*.ts ./node_modules/@pyxis/webpack/build/register/typescript.js ./src/higher/run"
  },
  "lint-staged": {
    "{src}/**/*.ts*": [
      "prettier --write",
      "git add"
