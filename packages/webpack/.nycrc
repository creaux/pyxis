{
  "reporter": [
    "text",
    "html"
  ],
  "require": [
    "./register.js"
  ],
  "extension": [
    ".ts"
  ],
  "all": true,
  "exclude": [
    "src/**/spec.ts",
    "src/**/index.ts",
    "src/**/*.d.ts"
  ],
  "include": [
    "src/**/*.ts"
  ],
  "cache": false,
  "sourceMap": false,
  "instrument": false,
  "check-coverage": true,
  "lines": 100,
  "statements": 100,
  "functions": 100,
  "branches": 92
}
