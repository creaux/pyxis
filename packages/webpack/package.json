  "directories": {
    "lib": "build"
  },
  "files": [
    "**/*"
  ],
  "scripts": {
    "build": "node webpack.register.js && yarn declarations",
    "build:watch": "webpack --config webpack.config.babel.js --watch",
    "pretest": "yarn typing && yarn lint",
    "test": "NODE_ENV=test nyc ava --verbose",
    "test:watch": "ava --watch",
    "precommit": "lint-staged",
    "report": "nyc report",
    "lint": "tslint -c tslint.json ./src/**/*.ts",
    "typing": "tsc --noEmit",
    "declarations": "tsc --emitDeclarationOnly --declaration --noEmit false --allowJs false --outDir build/declarations && dts-bundle --configJson dts-bundle.json"
  },
  "lint-staged": {
    "{src}/**/*.ts*": [
      "prettier --write",
      "git add"
    ]
  },
