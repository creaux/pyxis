{
  "name": "@pyxis/keystone",
  "version": "0.0.1",
  "private": true,
  "engines": {
    "node": "10.13.0",
    "npm": "5.3.0",
    "yarn": "1.10.1",
    "mongodb": "3.4"
  },
  "scripts": {
    "lint": "eslint app --ext ts --config .eslintrc.json",
    "predev": "NODE_ENV=development ts-node app/scripts/drop",
    "dev": "NODE_ENV=development nodemon --exec ts-node -- app/app.ts",
    "start": "ts-node app/app.ts",
    "test": "npm run test:nyan",
    "test:diff": "ava | tap-diff",
    "test:nyan": "ava | tnyan",
    "test:coverage": "nyc ava",
    "watch:test": "ava --watch",
    "types": "tsc",
    "precommit": "lint-staged",
    "mongo": "mongod --dbpath /var/db/mongo"
  },
  "dependencies": {
    "apollo-server-express": "2.0.6",
    "body-parser": "1.18.2",
    "graphql": "14.0.2",
    "graphql-rxjs": "0.11.7-0",
    "graphql-subscriptions": "0.5.4",
    "graphql-tools": "2.2.1",
    "iterall": "^1.1.3",
    "jade": "^1.11.0",
    "keystone": "4.0.0",
    "keystone-storage-adapter-s3": "^2.0.0",
    "mongoose": "4.5.10",
    "rxjs": "5.4.3",
    "underscore": "1.8.3"
  },
  "devDependencies": {
    "@types/graphql": "14.0.0",
    "@types/node": "^10.12.18",
    "@typescript-eslint/parser": "^1.1.0",
    "ava": "^1.2.0",
    "eslint": "5.12.1",
    "eslint-config-airbnb": "17.1.0",
    "eslint-plugin-import": "2.15.0",
    "eslint-plugin-jsx-a11y": "6.2.0",
    "eslint-plugin-react": "7.12.4",
    "husky": "0.14.3",
    "lint-staged": "7.2.2",
    "nodemon": "1.18.9",
    "nyc": "11.2.1",
    "prettier": "^1.16.1",
    "sinon": "4.0.1",
    "tap-diff": "0.1.1",
    "tap-nyan": "1.1.0",
    "ts-node": "^8.0.2",
    "typescript": "3.2.4"
  },
  "lint-staged": {
    "app/**/*.ts*": [
      "prettier --write",
      "git add"
    ]
  }
}
